[Unit]
Description=msgd - Simple UDP-based message reception service
Before=graphical-session-pre.target
ConditionPathExists=/etc/X11/Xsession.options
Wants=dbus.socket
After=dbus.socket

[Service]
ExecStart=/home/owen/python/msgd/msgd --logfile /home/owen/python/msgd/msgd.log --msglog /home/owen/python/msgd/messages
#ExecStopPost=/usr/lib/openssh/agent-launch stop
